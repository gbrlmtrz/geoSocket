{
  "name": "geosocket",
  "version": "0.0.1",
  "description": "Geolocation based socket with pub/sub functionality",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "start:app": "ng serve --port 8080 --host 0.0.0.0 --ssl true --ssl-cert \"ssl/server.crt\" --ssl-key \"ssl/server.key\"",
    "startProd": "set NODE_ENV=production && node index.js",
    "build:compresor": "webpack --config src/angularApp/compresor/webpack.config.js",
    "build:socketClient": "webpack --config src/angularApp/socketClient/webpack.config.js",
    "build:audioRecorder": "webpack --config src/angularApp/audioRecorder/webpack.config.js",
    "build:app": "ng build --base-href https://app.sixdegreesnear.com/ --aot true --prod --extract-css true --named-chunks false --buildOptimizer && node build/fix-sw --base-href https://app.sixdegreesnear.com/",
    "test": "ng test",
    "ng": "ng",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
